{
  "name": "node.js",
  "version": "0.0.1",
  "description": "Node.js backend application",
  "main": "dist/app.js",
  "private": false,
  "scripts": {
    "dev": "nodemon --exec babel-node src/app.js",
    "build": "rm -rf dist/ && babel src --out-dir dist/",
    "start": "node dist/app.js",
    "sequelize": "babel-node ./node_modules/.bin/sequelize-cli $*"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "@hapi/joi": "^17.1.0",
    "@sentry/node": "^6.2.5",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "helmet": "^4.4.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-top-level-await": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/runtime": "^7.13.10",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/transform-runtime",
      "@babel/proposal-class-properties",
      "@babel/syntax-top-level-await"
    ]
  },
  "engine": {
    "node": ">=12.14.1",
    "npm": ">=6.12.2"
  },
  "license": "ISC"
}
