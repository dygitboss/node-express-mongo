{
  "name": "node.js",
  "version": "0.0.1",
  "description": "Node.js backend application",
  "main": "dist/app.js",
  "private": false,
  "scripts": {
    "dev": "nodemon --exec babel-node src/app.js",
    "build": "rm -rf dist/ && babel src --out-dir dist/",
    "start": "node dist/app.js",
    "sequelize": "babel-node ./node_modules/.bin/sequelize-cli $*"
  },
  "dependencies": {
    "@hapi/boom": "^9.0.0",
    "@hapi/joi": "^17.1.0",
    "@sentry/node": "^5.1.0",
    "bcrypt": "^4.0.0",
    "body-parser": "^1.18.3",
    "colors": "^1.3.3",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.16.4",
    "glob": "^7.1.6",
    "helmet": "^3.15.1",
    "mongoose": "^5.4.16",
    "mongoose-delete": "^0.5.1",
    "multer": "^1.4.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.5",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/transform-runtime",
      "@babel/proposal-class-properties"
    ]
  },
  "engine": {
    "node": "^12",
    "npm": "^6"
  },
  "license": "ISC"
}
